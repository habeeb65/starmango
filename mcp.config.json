{
  "include": [
    "Accounts/**/*.py",
    "Mango_project/**/*.py",
    "templates/**/*.html",
    "*.py"
  ],
  "exclude": [
    "**/venv/**",
    "**/migrations/**",
    "**/__pycache__/**",
    "**/node_modules/**",
    "**/.git/**"
  ],
  "indexing": {
    "maxTokens": 2000000,
    "tokensPerFile": 15000
  },
  "analysis": {
    "django": true,
    "model_files": ["Accounts/models.py"],
    "view_files": ["Accounts/views.py"],
    "admin_files": ["Accounts/admin.py"],
    "template_dirs": ["templates/"],
    "settings_files": ["Mango_project/settings.py"]
  }
} 